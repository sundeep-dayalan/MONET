{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@features/*": [
        "./src/app/features/*"
      ],
      "@locales/*": [
        "./src/locales/*"
      ],
      "@http/*": [
        "./src/http/*"
      ],
      "@components/*": [
        "./src/app/components/*"
      ],
      "@config/*": [
        "./src/app/config/*"
      ],
      "@hooks/*": [
        "./src/app/hooks/*"
      ],
      "@store/*": [
        "./src/app/store/*"
      ],
      "@app/types/*": [
        "./src/app/types/*"
      ],
      "@utils/*": [
        "./src/app/utils/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@api/*": [
        "./src/app/http/api/*"
      ]
    }
  }
}